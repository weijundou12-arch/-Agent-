{
  "$id": "m5_tools.json",
  "type": "object",
  "properties": {
    "request": {
      "type": "object",
      "properties": {
        "meta": { "$ref": "common.json#/definitions/RequestMeta" },
        "tool_name": { "type": "string" },
        "tool_version": { "type": "string" },
        "arguments": { "type": "object" }
      },
      "required": ["meta","tool_name","tool_version","arguments"]
    },
    "response": {
      "type": "object",
      "properties": {
        "ok": { "type": "boolean" },
        "result": { "type": "object" },
        "audit": { "type": "array", "items": { "$ref": "common.json#/definitions/AuditEvent" } }
      },
      "required": ["ok","result","audit"]
    }
  },
  "required": ["request","response"]
}
